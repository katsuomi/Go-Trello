{
  "name": "Go Trello",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Record the selected anchor tag content on trello card.",
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["index.js"]
  },
  "permissions": ["tabs", "contextMenus"]
}
